{
  "name": "busy-car",
  "version": "0.1.0",
  "description": "That project allows anyone to rent available great cars",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/** --color",
    "lint:fix": "eslint src/** --fix",
    "test": "npm run clean && jest --runInBand",
    "cover": "npm run clean && jest --coverage --runInBand",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "clean": "rimraf dist && rimraf coverage",
    "migration": "knex --env ${KNEX_ENV} --knexfile knexfile.js migrate:latest",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grrlopes/busy-car.git"
  },
  "keywords": [
    "cars",
    "rent",
    "shopcar"
  ],
  "author": "Gabriel Lopes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/grrlopes/busy-car/issues"
  },
  "homepage": "https://github.com/grrlopes/busy-car#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/knex": "^0.16.1",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.7",
    "pg": "^8.6.0",
    "pm2": "^5.1.0",
    "uuid": "^8.3.2"
  }
}
